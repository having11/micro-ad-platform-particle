{
    "extensions.ignoreRecommendations": true,
    "C_Cpp.default.configurationProvider": "particle.particle-vscode-core",
    "files.associations": {
        "*.ino": "cpp",
        "array": "cpp",
        "tuple": "cpp"
    },
    "cmake.configureOnOpen": false,
    "particle.firmwareVersion": "5.5.0",
    "particle.targetPlatform": "p2",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/lib"
    ],
    "C_Cpp.default.forcedInclude": [
        "${workspaceFolder}/lib/AnimatedGIF/src/AnimatedGIF.h"
    ],
    "particle.targetDevice": "photon2_1"
}